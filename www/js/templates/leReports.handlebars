<div>
	<div data-role="collapsibleset">
	{{#each reports}}
		<div data-role="collapsible">
			<h1>{{capitalize this.bikeInfo.model}} bike reported {{lowercase status}} on {{date}}.</h1>
			<div>
				<img class="small-pic ui-corner-all center side-by-side five-mar" src="img/bike2.jpg">
				<div class="side-by-side center">
					<button class="blue-button medium-button five-mar" onclick="toBikeDetail('{{bikeInfo}}')">Bike Details</button>
					<button class="blue-button medium-button center five-mar toEditReport" onclick="followUnfollow('{{reportID}}', '{{../following}}')">
						{{#if ../following}}Follow Report{{else}}Unfollow Report{{/if}}
					</button>
				</div>
			</div>
			<h3>Incident Information</h3>
			<table cellpadding="3px">
				<tr class="{{no incidentNumber 'hidden'}}">
					<td><b>Incident Number:</b></td>
					<td>{{incidentNumber}}</td>
				</tr>
				<tr class="{{no officerName 'hidden'}}">
					<td><b>Officer's Name:</b></td>
					<td>{{officerName}}</td>
				</tr>
				<tr class="{{no officerEmail 'hidden'}}">
					<td><b>Officer's Email:</b></td>
					<td>{{officerEmail}}</td>
				</tr>
				<tr class="{{no officerPhone 'hidden'}}">
					<td><b>Officer's Phone:</b></td>
					<td>{{officerPhone}}</td>
				</tr>
				<tr class="{{no status 'hidden'}}">
					<td><b>Reported:</b></td>
					<td>{{status}}</td>
				</tr>
				<tr class="{{no date 'hidden'}}">
					<td><b>Report Date:</b></td>
					<td>{{date}}</td>
				</tr>
				<tr class="{{no contactOwner 'hidden'}}">
					<td><b>When Found:</b></td>
					<td>{{contact contactOwner contactPolice}}</td>
				</tr>
				<tr class="{{no description 'hidden'}}">
					<td><b>Incident Description:</b></td>
					<td>{{description}}</td>
				</tr>
			</table>
		</div>
		{{/each}}
	</div>
</div>